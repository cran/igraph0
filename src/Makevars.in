PKG_CPPFLAGS=-DNDEBUG @CPPFLAGS@
PKG_CFLAGS=-DUSING_R -I. @CFLAGS@ \
        -DPACKAGE_VERSION=\"@PACKAGE_VERSION@\" \
	-DINTERNAL_ARPACK -DINTERNAL_LAPACK \
	-DINTERNAL_BLAS -DNDEBUG $(CFLAGS)
PKG_CXXFLAGS= -DUSING_R -DNDEBUG
PKG_LIBS=@XML2_LIBS@ @GMP_LIBS@

all: $(SHLIB)

dlamch.o : dlamch.c
	$(CC) $(R_XTRA_FFLAGS) $(PKG_CFLAGS) \
	$(FPICFLAGS) $(SHLIB_CFLAGS) $(SAFE_FFLAGS) $(CFLAGS) -c $<
